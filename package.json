{
  "name": "Project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "npm run prettier:check && npm run eslint:check && npm run type:check",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "type:check": "tsc",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky",
    "update": "npx npm-check-updates -i",
    "docs": "npm start --prefix ./docs",
    "docs:install": "npm install --prefix ./docs",
    "docs:clean-install": "npm ci --prefix ./docs",
    "docs:build": "npm run build --prefix ./docs",
    "storybook": "concurrently -n web,mobile -c magentaBright,greenBright -k \"npm run storybook:web\" \"npm run storybook:device\"",
    "storybook:device": "STORYBOOK=true npm start",
    "storybook:device:generate": "sb-rn-get-stories --config-path .storybook.ondevice",
    "storybook:web": "storybook dev -p 6006",
    "storybook:web:build": "storybook build -o docs/static/storybook-static"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.77.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.7",
    "@babel/preset-env": "7.26.7",
    "@babel/runtime": "7.26.7",
    "@chromatic-com/storybook": "3.2.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/js": "9.19.0",
    "@gorhom/bottom-sheet": "5.1.0",
    "@react-native-async-storage/async-storage": "2.1.1",
    "@react-native-community/cli": "15.1.3",
    "@react-native-community/cli-platform-android": "15.1.3",
    "@react-native-community/cli-platform-ios": "15.1.3",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/slider": "4.5.5",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-links": "8.5.3",
    "@storybook/addon-ondevice-actions": "8.5.3",
    "@storybook/addon-ondevice-controls": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/react-native": "8.5.3",
    "@storybook/react-native-web-vite": "8.5.6",
    "@types/jest": "29.5.14",
    "@types/react": "18.2.6",
    "@types/react-test-renderer": "18.0.0",
    "babel-loader": "8.4.1",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "commitizen": "4.3.1",
    "commitlint": "19.6.1",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-perfectionist": "4.7.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.18",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.4.2",
    "prettier": "3.4.2",
    "react-dom": "18.3.1",
    "react-native-gesture-handler": "2.23.0",
    "react-native-reanimated": "3.16.7",
