{
  "name": "Project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "npm run clear:watchman",
    "start": "ENV=local npm run react-native",
    "pretest": "npm run clear:watchman",
    "test": "jest",
    "react-native": "react-native start --reset-cache --client-logs",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "npm run prettier:check && npm run eslint:check && npm run type:check",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "type:check": "tsc",
    "prepare": "husky",
    "update": "npx npm-check-updates -i",
    "docs": "npm start --prefix ./docs",
    "docs:install": "npm install --prefix ./docs",
    "docs:clean-install": "npm ci --prefix ./docs",
    "docs:build": "npm run build --prefix ./docs",
    "storybook": "concurrently -n web,mobile -c magentaBright,greenBright -k \"npm run storybook:web\" \"npm run storybook:device\"",
    "prestorybook:device": "npm run clear:watchman",
    "storybook:device": "STORYBOOK=true npm run react-native",
    "storybook:device:generate": "sb-rn-get-stories --config-path .storybook.ondevice",
    "storybook:web": "storybook dev -p 6006",
    "storybook:web:build": "storybook build -o docs/static/storybook-static",
    "clear:watchman": "watchman watch-del '.' ; watchman watch-project '.'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@reduxjs/toolkit": "2.6.0",
    "date-fns": "4.1.0",
